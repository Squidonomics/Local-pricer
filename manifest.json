{
    "manifest_version": 3,
    "name": "Local Pricer",
    "version": "1.0.0",
    "icons": {
        
    },
    "action": {
        "default_popup": "popover.html"
    },
    "permissions": [
        "storage"
      ],
      "background": {
        "service_worker": "service-worker.js"
      },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://*/*"
            ]
        }
    ]
}